# Frontend Environment Configuration

# Backend API URL
REACT_APP_API_URL=http://localhost:8080

# Application Environment
REACT_APP_ENV=development

# Enable/disable debugging
REACT_APP_DEBUG=true

# API timeout in milliseconds
REACT_APP_API_TIMEOUT=10000

# Maximum number of toast notifications
REACT_APP_MAX_TOASTS=5

# Auto-refresh interval for dashboard (in seconds)
REACT_APP_REFRESH_INTERVAL=30

# Default page size for data tables
REACT_APP_DEFAULT_PAGE_SIZE=10

# Enable/disable service worker
REACT_APP_ENABLE_SERVICE_WORKER=false
